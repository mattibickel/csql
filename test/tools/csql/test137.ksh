#!/bin/sh
# TEST DEFAULT NULL For NOT NULL Field 
#

QUITFILE=${PWD}/tools/csql/quit.sql
REL_PATH=.
if [ -s "$QUITFILE" ]
then
    REL_PATH=`pwd`/tools/csql
fi

$CSQL_INSTALL_ROOT/bin/csql -u root -p manager -s ${REL_PATH}/default_NULL_on_NOT_NULL.sql
if [ $? -ne 0 ]
then
   $CSQL_INSTALL_ROOT/bin/csql -u root -p manager -s ${REL_PATH}/drop.sql
   rm -f ${REL_PATH}/drp1.sql
   exit 1;
fi

exit 0;
