CREATE TABLE t1 (f1 INT  NOT NULL , f2 INT );
CREATE INDEX t1_idx1_Primary on t1 (  f1  )  HASH  UNIQUE;
CREATE TABLE t2 (f3 INT  NOT NULL , f4 INT );
