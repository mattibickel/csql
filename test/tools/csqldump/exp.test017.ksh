CREATE TABLE t1 (f1 INT  NOT NULL , f2 INT );
CREATE INDEX t1_idx1_Primary on t1 (  f1  )  HASH  UNIQUE;
CREATE TABLE t2 (f3 INT , f4 INT  NOT NULL , FOREIGN KEY ( f3 ) REFERENCES t1 ( f1 ));
CREATE INDEX t2_idx1_Primary on t2 (  f4  )  HASH  UNIQUE;
CREATE TABLE t3 (f5 INT , f6 INT , FOREIGN KEY ( f5 ) REFERENCES t2 ( f4 ));
